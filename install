if [[ $(lsb_release -rs) == "18.04" ]]; then

   echo "install for 18.04"
   cd installers/ubuntu-18
   source ./install
   
elif [[ $(lsb_release -rs) == "20.04" ]]; then

   echo "install for 20.04"
   cd installers/ubuntu-20
   source ./install

else

   lsb_release -rs
   echo "This ubuntu version is not supported."
       
fi
